	<parameter name="codigo_productor" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[WITH 
saldoConProductor AS 
(
SELECT codigo_productor, SUM(saldo) as saldo, SUM(cantidad_miel) as miel
FROM cta_cte_productor
where codigo_productor = 5
)
select c.codigo_productor, r.nombre, c.descripcion_movimiento, s.saldo, s.miel from cta_cte_productor c join saldoConProductor s on c.codigo_productor = s.codigo_productor
join productor p on c.codigo_productor = p.cod_productor
join persona r on p.cod_persona = r.cod_persona]]>
	</queryString>
